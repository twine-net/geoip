{
    "name": "pulkitjalan/geoip",
    "description": "Geoip Wrapper with Laravel Support",
    "homepage": "https://github.com/pulkitjalan/geoip",
    "keywords": [
        "geoip",
        "ipstack",
        "ip-api",
        "maxmind",
        "telize"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pulkit Jalan"
        }
    ],
    "require": {
        "php": "^5|^7",
        "illuminate/support": "~5|^6",
        "illuminate/console": "~5|^6",
        "guzzlehttp/guzzle": "^6",
        "geoip2/geoip2": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "PulkitJalan\\GeoIP\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "laravel": {
            "providers": [
                "PulkitJalan\\GeoIP\\GeoIPServiceProvider"
            ],
            "aliases": {
                "GeoIP": "PulkitJalan\\GeoIP\\Facades\\GeoIP"
            }
        }
    }
}
